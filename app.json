{
  "expo": {
    "name": "snappblock",
    "slug": "snappblock",
    "platforms": [
      "web",
      "ios"
    ],
    "version": "1.0.25",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "snappblock",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.25",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page.",
        "NSPhotoLibraryUsageDescription": "This app uses the camera to let user put a photo in his profile page."
      },
      "bundleIdentifier": "org.team0xnan.snappblock"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.team0xnan.snappblock",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ],
      "versionCode": 36
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "description": "Snappblock is a camera app rides on blockchain technology.",
    "githubUrl": "https://github.com/0xnan-dev/snappblock-app"
  },
  "plugins": [
    "sentry-expo"
  ]
}