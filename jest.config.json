{
  "preset": "jest-expo",
  "transformIgnorePatterns": [
    "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
  ],
  "moduleNameMapper": {
    "\\.svg": "<rootDir>/src/__mocks__/svgMock.js"
  },
  "moduleFileExtensions": [
    "tsx",
    "ts",
    "js"
  ],
  "automock": false
}